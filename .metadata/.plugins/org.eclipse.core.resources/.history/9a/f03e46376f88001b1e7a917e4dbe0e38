import java.util.Arrays;
import java.util.Scanner;

public class B {
	public static void main(String[] args) {

		int[] arr = {1, 2};

		System.out.println(arr[1]);
		
		int temp = 0;
		arr[0] = arr[1];
		arr[1] = temp;
		temp = arr[0];
		
		System.out.println(arr[1]);
		System.out.println(Arrays.toString(arr));
		
		int x = 1;
		int y = 2;
		int tmp = 0;
		
		x = y;
		y = tmp;
		tmp = x;
		
		System.out.println(x);
		System.out.println(y);
	}
}
