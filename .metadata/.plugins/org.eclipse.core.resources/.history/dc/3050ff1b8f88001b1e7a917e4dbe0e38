import java.util.Arrays;
import java.util.Scanner;

public class B {
	public static void main(String[] args) {

		int[] arr = {5,4,3,1,2};
		int tmp;
		
		for (int i=0; i<arr.length-1; i++) {
			int min_idx = i;
			for (int j=i; j<arr.length-1; j++) {
				if (arr[j] > arr[j+1]) {
					min_idx = j+1;
				}
			}
			tmp = arr[i];
			arr[i] = arr[min_idx];
			arr[min_idx] = tmp;
		}
		
		System.out.println(Arrays.toString(arr));
		

	}
}
