package array;

import java.util.Arrays;

public class Array_2 {

	public static void main(String[] args) {
		
		// 배열의 foreach문 활용과 sort
		int[] arr = {11,22,33,44, 7};
		System.out.println(Arrays.toString(arr));
		for(int value : arr) {
			System.out.print(value + ",");
		}
		System.out.println();
		
		
		// 오름차순, 내림차순 정렬
		Arrays.sort(arr);		

		
		
	}
}
